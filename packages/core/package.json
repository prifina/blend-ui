    "@styled-system/theme-get": "^5.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-popper": "^2.2.3",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^6.0.16",
    "@storybook/addon-storyshots": "^6.0.16",
    "@storybook/react": "^6.0.16",
